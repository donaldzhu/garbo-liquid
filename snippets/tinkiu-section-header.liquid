<div class="tinkiu-section-header">
  {% render 'tinkiu-dot'
    , withSpace: withSpace %}
  {% if title %}
    <h1>
      {{- title | escape -}}
    </h1>
  {% endif %}
  {% if smallTitle %}
    <h3>{{ smallTitle }}</h3>
  {% endif %}
  {% if secondaryTitle %}
    <h4>{{ secondaryTitle }}</h4>
  {% endif %}
</div>